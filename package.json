{
  "name": "vue-chartlib",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:docs": "vue-cli-service build --target app ./docs/main.ts",
    "build:lib": "vue-cli-service build --target lib --formats commonjs --name chartlib ./src/index.ts --no-clean --report && yarn rename:lib",
    "build:support": "vue-cli-service build --target lib  --formats commonjs --name support ./support/index.ts --no-clean --report && yarn rename:support",
    "storybook": "start-storybook -s ./public -p 9001 -c .storybook --ci",
    "test:unit": "vue-cli-service test:unit",
    "rename:lib": "node build/rename.lib.js",
    "rename:support": "node build/rename.support.js"
  },
  "main": "dist/chartlib.js",
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "dependencies": {
    "@quasar/extras": "^1.1.1",
    "brace": "^0.11.1",
    "lodash": "^4.17.11",
    "quasar": "^1.0.0-rc.4",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-backgrounds": "^5.1.3",
    "@storybook/addon-centered": "^5.1.3",
    "@storybook/addon-info": "^5.1.3",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addon-notes": "^5.1.3",
    "@storybook/addon-options": "^5.1.3",
    "@storybook/addon-storyshots": "^5.1.3",
    "@storybook/addon-storysource": "^5.1.3",
    "@storybook/addon-viewport": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/theming": "^5.1.3",
    "@storybook/vue": "^5.1.3",
    "@types/chai": "^4.1.0",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-plugin-unit-mocha": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "echarts": "^4.1.0",
    "echarts-gl": "^1.1.1",
    "echarts-liquidfill": "^2.0.4",
    "eslint": "^5.8.0",
