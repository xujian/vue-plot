    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.5.1",
    "vue-cli-plugin-quasar": "^1.0.0-rc.1",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.3.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.8"
  },
  "peerDependencies": {
    "echarts": ">4.0.0",
    "echarts-gl": "^1.1.1",
    "echarts-liquidfill": "^2.0.4"
  },
  "types": "types/index.d.js"
}
